#: ../winconn/WinconnWindow.py:54
msgid "xfreerdp died unexpectedly"
msgstr "грешка при изпълнението на xfreerdp"

#: ../winconn/WinconnWindow.py:55
msgid "Success"
msgstr "Успешна операция"

